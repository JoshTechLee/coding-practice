//Primitive data types
byte bits = 8;
short bits = 16;
int bits = 32;
long bits = 64;
float bits = 32.0;
double bits = 64.0;
char data = 'a'
String data = 'alphabet'
boolean flag = true;

//Loops
int n = 8;
for (int i = 0; i < n; i++){
  // do something "n" times
}
while (i < n){
  // do something "n" times
  i++;
}

