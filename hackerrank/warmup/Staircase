import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    static void staircase (int n){
        for (int i = 0; i < n; i++){
            System.out.format("%" + (n+1) + "s", "" + String.format("%0" + (i+1) + "d", 0).replace("0","#") + "\n");
        }
    }

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        staircase(n);
    }
}
